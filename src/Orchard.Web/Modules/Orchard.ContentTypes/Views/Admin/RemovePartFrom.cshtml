@model Orchard.ContentTypes.ViewModels.RemovePartViewModel
@{
    //Style.Require("ContentTypesAdmin");
    
}
<h1>@RenderTitlePart(T["Remove the \"{0}\" part from  \"{1}\"", Model.Name, Model.Type.DisplayName])</h1>

<form asp-action="RemovePartFrom">
    <p>@T["Looks like you couldn't use the fancy way to remove the part. Try hitting the button below to force the issue."]</p>
    <fieldset>
        <input asp-for="Name" type="hidden" />
        <button class="btn btn-primary" type="submit">@T["Remove"]</button>
    </fieldset>
</form>