@{ 
    IEnumerable<dynamic> contentItems = Model.ContentItems;
}
<p>
    <a class="btn btn-primary" role="button" asp-action="Create" asp-controller="Admin" asp-route-id="BlogPost" asp-route-area="Orchard.Contents" asp-route-ListPart.ContainerId="@Model.ContentItem.ContentItemId">@T.Html("Create new element")</a>
</p>

@if (contentItems.Any())
{
<ul class="list-group">
    @foreach (var contentItemSummary in contentItems)
    {
        <li class="list-group-item">
            @Display(contentItemSummary)
        </li>
    }
</ul>
}
else
{
    <p class="alert alert-warning">@T.Html("The list is empty")</p>
}