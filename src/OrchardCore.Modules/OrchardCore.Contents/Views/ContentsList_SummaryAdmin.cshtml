@using OrchardCore.ContentManagement
@inject IContentManager ContentManager

@{
    ContentItem contentItem = Model.ContentItem;
}

@if (contentItem.Content["ListPart"] != null)
{
    <small class="badge ta-badge font-weight-normal" data-toggle="tooltip" title="@T["List"]">
        <i class="fa fa-list text-secondary"></i> @T["List"]
    </small>
}
